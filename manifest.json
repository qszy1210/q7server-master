{
    "manifest_version": 2,
    "name": "q7-server-master",
    "version": "1.1.0",
    "description": "运维有些地方有点难用",
    "icons": {
        "16": "img/icon.png",
        "48": "img/icon.png",
        "128": "img/icon.png"
    },
    "background": {
        "page": "background.html"
    },
    "browser_action": {
        "default_icon": "img/icon.png",
        "default_title": "save your life to check server info",
        "default_popup": "popup.html"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["js/jquery-1.8.3.js"],
            "run_at": "document_start"
        }
    ],
    "permissions": [
        "unlimitedStorage",
        "webRequest",
        "storage",
        "cookies",
        "http://*/*",
        "https://*/*"
    ],
    "homepage_url": "http://ops.q7link.com:8080/",
    "options_ui": {
        "page": "options.html",
        "chrome_style": true
    },
    "commands": {
        "open_url": {
            "suggested_key": {
                "default": "Ctrl+2",
                "mac": "Command+2"
            },
            "description": "open url"
        },
        "_execute_browser_action": {
            "suggested_key": {
                "windows": "Ctrl+K",
                "mac": "Command+K",
                "chromeos": "Ctrl+K",
                "linux": "Ctrl+K"
            },
            "description": "default"
        }
    },
    "omnibox": {"keyword": "qs"}

}